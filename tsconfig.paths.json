{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@public/*": ["src/public/*"],
      "@app/*": ["src/app/*"],
      "@components/*": ["src/app/components/*"],
      "@services/*": ["src/app/services/*"],
      "@utils/*": ["src/app/utils/*"],
      "@apis/*": ["src/app/apis/*"],
      "@appTypes/*": ["src/app/types/*"],
      "@customHooks/*": ["src/app/customHooks/*"],
      "@routers/*": ["src/app/routers/*"],
      "@constants/*": ["src/app/constants/*"],
      "@ts/*": ["src/app/types/*"]
    }
  }
}
